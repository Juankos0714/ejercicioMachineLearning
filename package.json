{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "serve -s dist -l ${PORT:-10000} --no-clipboard",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "train": "tsx scripts/trainModels.ts train",
    "train:mock": "tsx scripts/trainModels.ts train:mock",
    "train:retrain": "tsx scripts/trainModels.ts train:retrain",
    "train:check": "tsx scripts/trainModels.ts check",
    "train:api": "tsx scripts/trainModels.ts api",
    "test:effectiveness": "tsx scripts/runEffectivenessTest.ts",
    "test:effectiveness:demo": "tsx scripts/runEffectivenessTestDemo.ts",
    "betting:demo": "tsx scripts/runBettingIntegrationDemo.ts",
    "markets:live": "tsx scripts/runLiveMarkets.ts",
    "markets:live:epl": "tsx scripts/runLiveMarkets.ts -- --sport=soccer_epl",
    "markets:live:laliga": "tsx scripts/runLiveMarkets.ts -- --sport=soccer_spain_la_liga",
    "markets:live:nba": "tsx scripts/runLiveMarkets.ts -- --sport=basketball_nba"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@tensorflow/tfjs": "^4.22.0",
    "lucide-react": "^0.344.0",
    "ml-cart": "^2.1.1",
    "ml-random-forest": "^2.1.0",
    "plotly.js-dist-min": "^3.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-plotly.js": "^2.6.0",
    "serve": "^14.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^24.10.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-plotly.js": "^2.6.3",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/ui": "^4.0.7",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^27.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.1",
    "tsx": "^4.20.6",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^4.0.7"
  }
}
